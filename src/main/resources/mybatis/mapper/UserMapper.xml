<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.attend.dao.UserMapper">
    <select id="getUserList" resultType="com.example.attend.dto.user">
        SELECT * FROM users;
    </select>

    <select id="getSamuel" resultType="com.example.attend.dto.user">
        SELECT * FROM users where name ='samuel';
    </select>
</mapper>

